    "lucide-react-native": "^0.244.0"
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.1.0",
    "mongoose": "^7.6.3",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/express": "^4.17.20",
    "@types/node": "^20.9.0",
    "@types/react": "~18.2.45",
    "@types/react-native": "~0.74.1",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "typescript": "~5.3.3"
